issn = {1353-4505},
journal = {International journal for quality in health care : journal of the International Society for Quality in Health Care / ISQua},
keywords = {hospital work environment,patient questionnaire,quality of care},
number = {6},
pages = {555--566},
pmid = {9007605},
title = {{The development and application of a patient satisfaction measurement system for hospital-wide quality improvement.}},
volume = {8},
year = {1996}
}
