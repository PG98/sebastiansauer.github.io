isbn = {1353-4505},
issn = {13534505},
journal = {International Journal for Quality in Health Care},
keywords = {Patient questionnaire,Patient satisfaction,Quality from the patient's perspective,Quality satisfaction performance},
number = {3},
pages = {257--264},
