number = {4},
pages = {506--551},
publisher = {Pergamon},
title = {{Gray's Behavioural Inhibition System as a mediator of mindfulness towards well-being}},
url = {http://dx.doi.org/10.1016/j.paid.2010.11.019},
volume = {50},
