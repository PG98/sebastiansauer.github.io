month = {feb},
number = {1},
pages = {39--56},
pmid = {20013298},
title = {{Occipital gamma activation during Vipassana meditation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2812711&tool=pmcentrez&rendertype=abstract},
