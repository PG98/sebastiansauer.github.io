isbn = {0000000000000},
issn = {1438-8812},
journal = {Endoscopy},
keywords = {analysis of change,dr,extreme groups,greifswald,hermann faller,in der rehabilitationsforschung,koordinatoren der reihe ¹methoden,prof,regression to the mean,thomas kohlmann,w{\"{u}}rzburg},
number = {July},
