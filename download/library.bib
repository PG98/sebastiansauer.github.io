@article{RoedigerIII2006,
abstract = {—A powerful way of improving one's memory for material is to be tested on that material. Tests enhance later retention more than additional study of the material, even when tests are given without feedback. This surpris- ing phenomenon is called the testing effect, and although it has been studied by cognitive psychologists sporadically over the years, today there is a renewed effort to learn why testing is effective and to apply testing in educational settings. In this article, we selectively review laboratory studies that reveal the power of testing in improving re- tention and then turn to studies that demonstrate the basic effects in educational settings. We also consider the related concepts of dynamic testing and formative assess- ment as other means of using tests to improve learning. Finally,we consider some negative consequences of testing that may occur in certain circumstances, though these negative effects are often small and do not cancel out the large positive effects of testing. Frequent testing in the classroom may boost educational achievement at all levels of education.},
author = {{Roediger III}, Henry L. and Karpicke, Jeffrey D.},
doi = {10.1111/j.1467-8721.2008.00612.x},
file = {:Users/sebastiansauer/Documents/Literatur/div/Studien_Unterricht/7. Ged{\~{N}}chtnis und Handlungseffekt/Literatur/Perspectives on Psychological Science-2006-Roediger-181-210.pdf:pdf},
