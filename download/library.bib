keywords = {Affect,Affect: physiology,Antibodies, Viral,Antibodies, Viral: physiology,Brain,Brain Mapping,Brain: physiology,Electrophysiology,Emotions,Emotions: physiology,Humans,Hydrocortisone,Hydrocortisone: metabolism,Influenza Vaccines,Neurosciences,Neurosciences: trends},
month = {sep},
number = {1449},
pages = {1395--411},
pmid = {15347531},
title = {{Well-being and affective style: neural substrates and biobehavioural correlates.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1693421&tool=pmcentrez&rendertype=abstract},
volume = {359},
