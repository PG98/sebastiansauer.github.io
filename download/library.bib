journal = {Zeitschrift f{\"{u}}r Evidenz, Fortbildung und Qualit{\"{a}}t im Gesundheitswesen},
keywords = {a,an intervention outweigh the,consideration for the magnitude,downsides of an intervention,evidence-based medicine,for all patient-,grade,guidelines,level of evidence,of,recommendation for action requires,the desirable effects of,the expected benefit and,undesirable effects},
number = {6},
pages = {391--400},
title = {{GRADE: Von der Evidenz zur Empfehlung.Beschreibung des Systems und L{\"{o}}sungsbeitrag zur {\"{U}}bertragbarkeit von Studienergebnissen}},
