journal = {Proceedings of the SIGCHI conference on Human factors in computing systems celebrating interdependence - CHI '94},
keywords = {database query,dynamic queries,information seeking,starfield displays,tight coupling},
pages = {313--317},
title = {{Visual information seeking}},
url = {http://dl.acm.org/citation.cfm?id=191666.191775},
year = {1994}
