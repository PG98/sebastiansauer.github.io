@article{Pivik1970,
abstract = {The occurrence of phasic reductions of EMG activity during non-rapid eye movement (non-REM) sleep in man and cat are described, and an analysis of the phenomenon in man is presented. Twelve subjects (ten male, two female) slept a total of 107 nights in the sleep laboratory. Standard recordings were taken of electroencephalographic (EEG), electrooculographic (EOG), and electromyographic (EMG) activity. Suppressions of EMG were observed in all subjects during all stages of non-REM sleep, but occurred with the greatest frequency during stages 2 and 4. The suppressions averaged 250 msec in duration and exhibited a higher frequency in the 10 min immediately prior to REM periods than in the 10 min immediately subsequent to REM periods. The relationship between the H reflex and phasic EMG suppressions was examined in three subjects (two males, one female) who were recorded a total of 9 nights. In addition to standard EEG, EOG, and EMG derivations, the H reflex was elicited by electrical stimulation of the posterior tibial nerve, recorded from the soleus muscle, and viewed on an oscilloscope. The EEG, EMG, and soleus muscle action potentials were recorded on magnetic tape. In all subjects it was found that whenever a stimulus occurred coincidental with a phasic EMG reduction, the reflex response was invariably inhibited. The observation and verification of phasic activity during non-REM sleep in the human subject is related to recent data in the cat, and the application of this physiological event to studies of non-REM mentation is discussed.},
author = {Pivik, T. and Dement, W.C.},
doi = {10.1016/0014-4886(70)90206-2},
issn = {00144886},
journal = {Experimental Neurology},
month = {apr},
number = {1},
pages = {115--124},
title = {{Phasic changes in muscular and reflex activity during non-REM sleep}},
url = {http://www.sciencedirect.com/science/article/pii/0014488670902062},
volume = {27},
year = {1970}
}
@article{Sauer2012c,
abstract = {The experience of the present moment is characterized by an integrative mechanism that fuses successive events into a unitary phenomenological experience with a temporal limit of about 3. s. We hypothesized that proficiency of mindfulness expands the ability to stabilize an ambiguous percept in a bistable image paradigm using the Necker Cube, and that this effect is associated with individual differences in the level of mindfulness. Expanded duration of nowness as indicated by the ability to stabilize a bistable image stimulus for a longer period of time may improve cognitive resources and thus be of practical interest. In a sample of n= 38 meditators and n= 38 non-meditators, meditators showed longer duration of subjective nowness. This effect was associated with individual mindfulness levels. It is concluded that the subjective now can be longer for meditators than for non-meditators, and individual levels of mindfulness may convey this effect. {\textcopyright} 2012.},
