isbn = {1353-4505},
issn = {1353-4505, 1464-3677},
journal = {International Journal for Quality in Health Care},
keywords = {adverse events,incident reporting and analysis,patient safety},
number = {2},
pages = {105--113},
pmid = {22190588},
title = {{Adverse events in Spanish intensive care units: the SYREC study}},
url = {http://intqhc.oxfordjournals.org/content/24/2/105%5Cnhttp://intqhc.oxfordjournals.org/content/24/2/105.full.pdf%5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/22190588},
