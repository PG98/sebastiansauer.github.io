@article{Sauer2014d,
abstract = {Whereas the number of studies supporting the efficacy of mindfulness as a health intervention is increasing, the measurement of mindfulness remains a subject of debate. Given the importance of measurement in this field, this paper aims to further our understanding of the assessment of mindfulness by employing an approach referred to as “random forests” (RF). RF is an ensemble learning method that is based on decision trees. RF is well known in biological research, for example, but is practically unknown in psychometrics. In this study, RF was used to gauge the predictive validity of the items from two mindfulness instruments concerning their ability to estimate group allocation (i.e., mindfulness practitioners vs. nonpractitioners). To allow for a better generalization of the results, we examined the research questions in two samples (N=76 and N=202) of different quality. We investigated two instruments: the Freiburg Mindfulness Inventory (FMI) and the Mindfulness Attention and Awareness Scale. Although results indicated that both instruments were capable of distinguishing practitioners from nonpractitioners, the predictive quality of most items on both scales was determined to be insufficient.},
author = {Sauer, Sebastian and Lemke, Jana and Zinn, Winfried and Buettner, Ricardo and Kohls, Niko},
doi = {10.1016/j.paid.2014.09.011},
file = {:Users/sebastiansauer/Documents/Forschung/Achtsamkeit_Studien/Mindful_in_Random_Forest/Mindful_in_rand_for/ms/rev2/Sauer_etal_2014_Mindful_in_a_Random_Forest_PAID_postprint.pdf:pdf;:Users/sebastiansauer/Documents/Forschung/__Publikationen/sauer_etal_2014_random_forests_paid.pdf:pdf;:Users/sebastiansauer/Documents/Forschung/__Publikationen/Sauer_etal_2014_mindf_rand_for.pdf:pdf;:Users/sebastiansauer/Documents/Forschung/Achtsamkeit_Studien/Mindful_in_Random_Forest/Mindful_in_rand_for/ms/rev2/titlepage_Mindful_in_a_Random_Forest_v14.pdf:pdf},
issn = {01918869},
journal = {Personality and Individual Differences},
keywords = {Assessment,Measurement,Mindfulness,Psychometrics,Random forests},
