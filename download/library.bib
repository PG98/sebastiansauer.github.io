journal = {J Pain Symptom Manage},
keywords = {*Family,*Job Satisfaction,*Patient Satisfaction,*Quality of Health Care,Adult,Aged,Analysis of Variance,Data Collection,Data Interpretation, Statistical,Female,Hospices/manpower/*organization & administration,Humans,Male,Middle Aged,Young Adult},
