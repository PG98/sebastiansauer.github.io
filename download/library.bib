@article{Luque-Casado2013,
abstract = {In the present study, we investigated the relation between cognitive performance and heart rate variability as a function of fitness level. We measured the effect of three cognitive tasks (the psychomotor vigilance task, a temporal orienting task, and a duration discrimination task) on the heart rate variability of two groups of participants: a high-fit group and a low-fit group. Two major novel findings emerged from this study. First, the lowest values of heart rate variability were found during performance of the duration discrimination task, compared to the other two tasks. Second, the results showed a decrement in heart rate variability as a function of the time on task, although only in the low-fit group. Moreover, the high-fit group showed overall faster reaction times than the low-fit group in the psychomotor vigilance task, while there were not significant differences in performance between the two groups of participants in the other two cognitive tasks. In sum, our results highlighted the influence of cognitive processing on heart rate variability. Importantly, both behavioral and physiological results suggested that the main benefit obtained as a result of fitness level appeared to be associated with processes involving sustained attention.},
author = {Luque-Casado, Antonio and Zabala, Mikel and Morales, Esther and Mateo-March, Manuel and Sanabria, Daniel},
doi = {10.1371/journal.pone.0056935},
file = {:Users/sebastiansauer/Documents/Literatur/BioPsy/HRV/Luque-Casado_etal_2013_cogn_perf_hrv.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Adolescent,Adult,Analysis of Variance,Cognition,Heart Rate,Humans,Male,Physical Fitness,Psychomotor Performance,Young Adult},
number = {2},
pages = {e56935},
pmid = {23437276},
title = {{Cognitive performance and heart rate variability: the influence of fitness level.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3577676&tool=pmcentrez&rendertype=abstract},
volume = {8},
