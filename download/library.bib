file = {:Users/sebastiansauer/Documents/Literatur/Methoden_Literatur/Ver{\"{a}}nderungsmessung/Modern Statistical Techniques for the Analysis of Biomedical Data - Edwards-PedPulm2000.pdf:pdf},
