issn = {13534505},
journal = {International Journal for Quality in Health Care},
keywords = {GPs,Middle-income country,Primary care,Psychometrics,Quality questionnaire,Validation studies},
number = {1},
pages = {51--57},
pmid = {18927100},
title = {{Patients' perspective in Chilean primary care: A questionnaire validation study}},
volume = {21},
year = {2009}
}
