keywords = {and urban,care,elected in 1994,equality,health care provider,health policy,health status and health,inherited huge inequalities in,patient satisfaction,provision across racial,race,rural,s first democratic government,socioeconomic,socioeconomic status,south africa},
number = {6},
pages = {473--477},
pmid = {15985504},
title = {{Patient satisfaction with health care providers in South Africa: the influences of race and socioeconomic status.}},
volume = {17},
year = {2005}
}
