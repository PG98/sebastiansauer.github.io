issn = {2151-2426(Electronic);1015-5759(Print)},
journal = {European Journal of Psychological Assessment},
pages = {No Pagination Specified},
title = {{Mindful Attention Awareness in Spanish Palliative Care Professionals: Psychometric Study With IRT and CFA Models.}},
