@article{Farrington2000,
abstract = {\begin{description}\item{Background} The product-moment correlation\nr is widely used in criminology and psychiatry to measure strength\nof association. However, most criminological an dpsychiatric vairables\ncontravene its underlying assumptions.\item{Aim} To compare statistical\nmeasures of association based on dichotomous variables with the use\nof r.\item{Method} Explanatory variables for delinquency are investigated\nin the Pittsburgh Youth Study using a sample of 506 boys aged 13--14.\item{Results}\nDichotomization does not necessarily cause a decrease in measured\nstrength of associations. Conclusiongs about the most important explanatory\nvariables for delinquency were not greatly affected by using dichotomous\nas opposed to continuous variables, by different dichotomoization\nsplits, or by using logistic versus OLS multiple regression. Non-linear\nrelationships, interaction effects and multiple risk factor individuals\nwere easily studied using dichotomous data.\item{Conclusions} Dichotomization\nproduces meaningful findings that are easily understandable to a\nwide audience. Measures of association for dichotomous variables,\nsuch as the odds ratio, have many advantages and are often more realistic\nand meaningful measures of strength of relationship than the product-moment\ncorrelation r.},
author = {Farrington, David P. and Loeber, Rolf},
doi = {10.1002/cbm.349},
file = {:Users/sebastiansauer/Documents/Literatur/Methoden_Literatur/deskriptive_Stat/BeneDich.pdf:pdf},
issn = {0957-9664},
journal = {Criminal Behaviour and Mental Health},
number = {2},
