isbn = {1534-7796\r0033-3174},
issn = {0033-3174},
journal = {Psychosomatic medicine},
keywords = {Adult,Adult Cognition Female Ficusin Humans Male Meditat,Cognition,Cognition: physiology,Female,Ficusin,Ficusin: therapeutic use,Humans,Male,Meditation,Meditation: methods,Middle Aged,PUVA Therapy,Photosensitizing Agents,Photosensitizing Agents: therapeutic use,Phototherapy,Phototherapy: methods,Psoriasis,Psoriasis: diagnosis,Psoriasis: therapy,Psychological,Psychological Time Factors Treatment Outcome,Psychological: therapy,Severity of Illness Index,Stress,Time Factors,Treatment Outcome},
