@article{Solano-Flores2006,
abstract = {We contend that generalizability (G) theory allows design of psychometric approaches to testing English-language learners (EELs) that are consistent with current thinking in linguistics. We used G theory to estimate the amount of measurement error due to code (language or dialect). Fourth - and fifth-grade ELLs, native speakers of Haitian-Creole from two speech communities, were given the same set of mathematics items in the standard English and standard Haitian-Creole dialects (Sample 1) or in the standard and local dialects of Haitian-Creole (Samples 2 and 3). The largest measurement error observed was produced by the interaction of student, item and code. Our results indicate that the reliability and dependability of ELL achievement measures is affected by two facts that operate in combination: Each test item poses a unique set of linguistic challenges and each student has a unique set of linguistic strengths and weaknesses. This sensitivity to language appears to take place at the level of dialect. Also, students from different speech communities within the same broad linguistic group may differ considerably in the number of items needed to obtain dependable measures of their academic achievement. Whether students are tested in English or in their first language, dialect variation needs to be considered if language as a source of measurement error is to be effectively addressed.},
author = {Solano-Flores, Guillermo and Li, Min},
doi = {10.1111/j.1745-3992.2006.00048.x},
file = {:Users/sebastiansauer/Documents/Literatur/Methoden_Literatur/Messen/Genersalisierbarkeitstheorie/Solano-Flores & Li_The Use of Generalizability.pdf:pdf},
issn = {07311745},
journal = {Educational Measurement: Issues and Practice},
keywords = {English-language learners,generalizability theory,linguistic minorities,testing},
number = {1},
pages = {13--22},
title = {{The Use of Generalizability (G) Theory in the Testing of Linguistic Minorities}},
volume = {25},
year = {2006}
}
