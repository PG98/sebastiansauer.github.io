author = {de Haan, Gerhard and Kamp, Georg and Lerch, Achim and Martignon, Laura and M{\"{u}}ller-Christ, Georg and Nutzinger, Hans-G.},
booktitle = {Science And Technology},
doi = {10.1007/978-3-540-85492-0},
