journal = {International journal for quality in health care : journal of the International Society for Quality in Health Care / ISQua},
keywords = {Attitude of Health Personnel,France,Humans,Practice Guidelines as Topic,Questionnaires},
number = {3},
pages = {195--202},
pmid = {16484314},
title = {{Lack of validity of a French adaptation of a scale measuring attitudes towards clinical practice guidelines.}},
