journal = {PloS one},
keywords = {Biological Science Disciplines,Biological Science Disciplines: standards,Methods,Natural Science Disciplines,Natural Science Disciplines: standards,Publications,Publications: standards,Publications: trends,Research,Research: standards,Research: trends,Science,Science: classification,Science: methods,Science: standards,Social Sciences,Social Sciences: standards},
month = {jan},
number = {4},
pages = {e10068},
pmid = {20383332},
title = {{"Positive" results increase down the Hierarchy of the Sciences.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2850928&tool=pmcentrez&rendertype=abstract},
volume = {5},
year = {2010}
}
