@article{Moran2001,
abstract = {Reducing the number of items in a health-related quality of life instrument
will enhance efficiency. However, it is important to maintain measurement
properties. We determined the effect of reducing items from each
domain (dyspnea, fatigue, emotion and mastery) of the 20-item Chronic
Respiratory Questionnaire (CRQ). Three randomized trials of respiratory
rehabilitation provided data. We removed items one at a time from
each domain in three orders: by item impact, item responsiveness,
and at random. Responsiveness, test-retest reliability and construct
validity were evaluated at each step. Responsiveness and reliability,
evaluated by intraclass correlation coefficients (ICC), were reduced
marginally as the number of items was reduced to two items per domain.
The deterioration was greatest when reducing from two items to one.
To detect a particular effect, sample size would increase by about
10% when reducing the number of items in a domain to 2. Construct
validity showed a more marked deterioration. Reducing to two items
per domain would maintain responsiveness and reliability of the CRQ
at an acceptable level, with a trade-off of reduced construct validity
and increase in sample size requirements.},
author = {Moran, L A and Guyatt, G H and Norman, G R},
journal = {Journal of Clinical Epidemiology},
keywords = {Factor Analysis,Obstructive Quality of life Quality-Adjusted Life,Statistical Health Care Surveys Humans Lung Disea},
number = {6},
pages = {571--579},
title = {{Establishing the minimal number of items for a responsive, valid, health-related quality of life instrument}},
volume = {54},
year = {2001}
}
@article{Matrices2012,
abstract = {Diagnostische Zielsetzung:Bei den Advanced Progressive Matrices (APM) handelt es sich um ein haeufig eingesetztes und bewaehrtes Verfahren zur Messung des sprachunabhaengigen logischen Denkens. Die APM gehoeren zur Verfahrensgruppe der Raven-Matrizen-Tests (RPM) und stellen in diesem Rahmen eine Version fuer ein ueberdurchschnittliches kognitives Leistungsniveau dar. Durch den Einsatz eines Zeitlimits kann die intellektuelle Effizienz der Probanden beurteilt werden.Aufbau:Die APM-Items bestehen aus geometrischen Figuren bzw. Mustern, in denen je ein Element ausgelassen wurde. Aus acht Antwortalternativen ist diejenige zu waehlen, die das Muster korrekt ergaenzt. Die Aufgaben sind nach Schwierigkeitsgrad geordnet und in zwei separaten Aufgabenheften zu den Aufgabensets 1 und 2 zusammengefasst. Das erste Set umfasst 12 Aufgaben (APM I), das zweite 36 Aufgaben (APM II). APM I wird entweder zur Vorbereitung des Einsatzes von APM II oder als Screeninginstrument eingesetzt. APM II ist lediglich bei Personen mit hinreichend hohem intellektuellen Leistungsniveau einzusetzen, um unnoetige Frustrationen zu vermeiden. Die gewaehlte Antwortalternative ist auf einem Durchschreibebogen bzw. in der computerisierten Version auf dem Monitor anzukreuzen.Grundlagen und Konstruktion:Die APM wurden 1943 zur Personalauslese im militaerischen Bereich eingesetzt und 1947 fuer den allgemeinen Gebrauch veroeffentlicht. Basierend auf Analysen der Aufgabenschwierigkeit wurden die Items nach ansteigender Schwierigkeit geordnet. Eine erste Revision der APM und eine Reduktion der urspruenglich 48 Items auf 36 Items (APM II) erfolgten 1962. In den aktuellen Auflagen des Verfahrens wurden die Items und ihre Reihenfolge nicht mehr modifiziert. Die mit den RPM gemessene Faehigkeit, Beziehungen zu stiften, wird von Raven als Faehigkeit beschrieben, aus vorgegebenen Informationen ein neues Verstaendnis bzw. neue Informationen abzuleiten.Empirische Pr{\"{u}}fung und G{\"{u}}tekriterien:Aktuelle Manuale fuer die APM liegen von Raven et al. (1998b) sowie Heller et al. (1998) vor. Beide Manuale zitieren zahlreiche Studien, die mit den APM und den RPM als Verfahrensgruppe durchgefuehrt worden sind.Reliabilit{\"{a}}t: Die Reliabilitaetskoeffizienten fuer APM I fallen deutlich niedriger aus als fuer APM II. Als Masse der internen Konsistenz ermittelten Raven et al. (1998b, S. 24) fuer die APM II Split-half-Reliabilitaetskoeffizienten zwischen .83 und .87, fuer APM I Werte um .73. Heller et al. (1998, S. 12) ermittelten Alphawerte zwischen .34 und .78 (APM I) sowie zwischen .70 und .88 (APM II; Schueler der Klassen 6 bis 12). Nach einem Intervall von 3 Monaten ermittelten Heller et al. (1998, S. 13) an Schuelerstichproben Retestreliabilitaetskoeffizienten zwischen .58 und .82.Validit{\"{a}}t: Verwiesen wird auf signifikante Korrelationen zwischen APM und anderen Verfahren zur Messung intellektueller Faehigkeiten. Die Korrelationen mit sprachfreien Verfahren (z.B. Wechsler-Handlungs-IQ: r = .55) liegen ueber denen mit sprachbezogenen Verfahren (z.B. Wechsler-Verbal-IQ: r = .42; 131 Schueler). Korrelationen zwischen APM und den quantitativen und nonverbalen Skalen des Kognitiven Faehigkeitstests (.26 bis .44) bestaetigen insbesondere die Komponente des analogen Denkens in den APM (98 hochbegabte Schueler der 9./10. Klasse Gymnasium; 59 Schueler der 10. Klasse Regelgymnasium). Mit der beim Zahlenverbindungstest zentralen Komponente der Informationsverarbeitungsgeschwindigkeit weisen die APM zwar signifikante, aber nicht sehr hohe Zusammenhaenge von .34 (APM I) bzw. .33 (APM II) auf (114 Schueler der 8./9. Klasse). Mit den Mathematikschulnoten der 8. bzw. 9. Klasse (223 Schueler) bzw. den Deutschnoten wiesen die APM Korrelationen in Hoehe von .34 (APM I) und .35(APM II) bzw. .23 (APM I) und .27 (APM II) auf. Hinweise auf eine Geschlechtsabhaengigkeit der APM-Ergebnisse liegen nicht vor.Normen: Die deutschen Referenzwerte (Prozentraenge) von Raven et al. (1998b) basieren auf einer Stichprobe von 1142 Personen (48.5% weiblich, 51,5% maennlich; 13-75 Jahre). Ein vorhandenes Zeitlimit sowie unterschiedliche Bildungsniveaus und Berufsgruppen werden beruecksichtigt. Die Normen der APM-Ausgabe von Heller et al. (1998) beruhen auf einer deutschsprachigen Stichprobe von 581 Studierenden, 921 Schuelern und 157 Senioren ueber 60 Jahren. Ferner werden Daten von Normierungsstichproben aus Australien, China, Deutschland, Grossbritannien, Neuseeland, Nordirland, Polen sowie den USA aufgefuehrt.},
author = {Matrices, Advanced Progressive},
file = {:Users/sebastiansauer/Documents/Literatur/Diff_Psy_Diagnostik/Lehrbuecher_Diff_Diag_Testtheorie/Lehrbuecher_Diff_Pers_Psy/Friedman, Schustack (2004) Pers{\"{o}}nlichkeit-Psy+Diff.Psy/Glossar.pdf:pdf},
pages = {71--76},
title = {{Advanced Progressive Matrices}},
year = {2012}
}
