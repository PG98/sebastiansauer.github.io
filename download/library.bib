@article{Karabatsos2005a,
abstract = {The multinomial (Dirichlet) model, derived from de Finetti's concept of\nexchangeability, is proposed as a general Bayesian framework to test\naxioms on data, in particular, deterministic axioms characterizing\ntheories of choice or measurement. For testing, the proposed framework\ndoes not require a deterministic axiom to be cast in a probabilistic\nform (e.g., casting deterministic transitivity as weak stochastic\ntransitivity). The generality of this framework is demonstrated through\nempirical tests of 16 different axioms, including transitivity,\nconsequence monotonicity, segregation, additivity of joint receipt,\nstochastic dominance, coalescing, restricted branch independence, double\ncancellation, triple cancellation, and the Thomsen condition. The model\ngeneralizes many previously proposed methods of axiom testing under\nmeasurement error, is analytically tractable, and provides a Bayesian\nframework for the random relation approach to probabilistic measurement\n(J. Math. Psychol. 40 (1996) 219). A hierarchical and nonparametric\ngeneralization of the model is discussed. (C) 2004 Elsevier Inc. All\nrights reserved.},
author = {Karabatsos, George},
doi = {10.1016/j.jmp.2004.11.001},
file = {:Users/sebastiansauer/Documents/Literatur/Methoden_Literatur/Messen/Conjoint_Measurement_etc/Karabatsos_2005.pdf:pdf},
issn = {0022-2496},
journal = {Journal of Mathematical Psychology},
keywords = {axiom testing,bayesian analysis,exchangeability},
month = {feb},
number = {1},
pages = {51--69},
title = {{The exchangeable multinomial model as an approach to testing deterministic axioms of choice and measurement}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022249604000835},
volume = {49},
year = {2005}
}
